    <!-- 配货单列表搜索 -->
    <sql id="distrSendPageWhere">
        <if test="status != null">
            and status in
            <foreach item="item" index="index" collection="status"
                     open="(" separator="," close=")">
                #{item}
            </foreach>
        </if>
        <if test="startTime != null and endTime != null">
            AND distr_date between #{startTime} and #{endTime}
        </if>
        <if test="requestUnitCode != null and requestUnitCode != ''">
            AND request_unit_code LIKE concat('%', #{requestUnitCode},'%')
        </if>
        <if test="requestUnitName != null and requestUnitName != ''">
            AND request_unit_name LIKE concat('%', #{requestUnitName},'%')
        </if>
        <if test="distrCode != null and distrCode != ''">
            AND code LIKE concat('%', #{distrCode},'%')
        </if>
        <if test="distrType != null">
            AND distr_type = #{distrType}
        </if>
        <if test="distrManName != null and distrManName != ''">
            AND distr_man_name LIKE concat('%', #{distrManName},'%')
        </if>
    </sql>


    <sql id="distrOutPageWhere">

        <if test="statusTwo != null">
        AND (status = #{statusTwo}  )
        </if>
        <if test="requestUnitCode != null and requestUnitCode != ''">
