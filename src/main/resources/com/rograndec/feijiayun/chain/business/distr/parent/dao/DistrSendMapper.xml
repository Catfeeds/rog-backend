    </sql>

    <select id="getDistrSendPage" resultMap="DistrVo">
        SELECT
        id,
        distr_date,
        code,
        request_unit_code,
        request_unit_name,
        distr_man_name,
        distr_type,
        real_amount_total,
        notax_real_amount_total,
        tax_amount_total,
        status,
        remark
        FROM saas_distr_send
        WHERE enterprise_id = #{enterpriseId}

        <if test="status != null">
            and status in
            <foreach item="item" index="index" collection="status"
                     open="(" separator="," close=")">
                #{item}
            </foreach>
        </if>
        <if test="startTime != null and endTime != null">
            AND distr_date between #{startTime} and #{endTime}
        </if>
        <if test="requestUnitCode != null and requestUnitCode != ''">
            AND request_unit_code LIKE concat('%', #{requestUnitCode},'%')
        </if>
        <if test="requestUnitName != null and requestUnitName != ''">
            AND request_unit_name LIKE concat('%', #{requestUnitName},'%')
        </if>
        <if test="distrCode != null and distrCode != ''">
            AND code LIKE concat('%', #{distrCode},'%')
        </if>
        <if test="distrType != null">
            AND distr_type = #{distrType}
        </if>
        <if test="distrManName != null and distrManName != ''">
            AND distr_man_name LIKE concat('%', #{distrManName},'%')
        </if>
        <choose>
            <when test="sort != null and sort!= '' ">
                order by ${sort}
            </when>
            <otherwise>
                order by id desc
            </otherwise>
        </choose>
    </select>

    <select id="getDistrSendOrDistrOutPage" resultMap="DistrVo">
        SELECT
        id,
        distr_date,
        code,
        request_unit_id,
        request_unit_code,
        request_unit_name,
        distr_man_name,
        distr_type,
        real_amount_total,
        notax_real_amount_total,
        tax_amount_total,
        status,
        remark
        FROM saas_distr_send
        WHERE enterprise_id = #{enterpriseId}
        <include refid="distrSendPageWhere"></include>
        UNION
        SELECT
        id,
        out_date AS distr_date,
        code,
        request_unit_id,
        request_unit_code,
        request_unit_name,
        send_man_name AS distr_man_name,
        distr_type,
        real_amount_total,
        notax_real_amount_total,
        tax_amount_total,
        status,
        remark
        FROM saas_distr_out
        WHERE enterprise_id = #{enterpriseId}
        <include refid="distrOutPageWhere"></include>
        <choose>
            <when test="sort != null and sort!= '' ">
                order by ${sort}
            </when>
            <otherwise>
                order by id desc
            </otherwise>
        </choose>
        limit #{pageNo},#{pageSize}
    </select>

